#!/bin/bash

UPATH=/home/igor/Test

for name in $(ls -l ~/Test | awk '/pub/{print $9}' | cut -d"_" -f1)
    do 
        ssh igor@192.33.33.17 sudo 'useradd -m $name mkdir /home/$name/.ssh'
        scp igor@192.33.33.10:$UPATH/$name_key.pub igor@192.33.33.17:/home/$name/.ssh
        cat $name_key >> known_hosts
    done

